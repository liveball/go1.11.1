	{"TestBoundaryLine", _test.TestBoundaryLine},

	{"TestNameAccessors", _test.TestNameAccessors},

	{"TestMultipart", _test.TestMultipart},

	{"TestMultipartOnlyNewlines", _test.TestMultipartOnlyNewlines},

