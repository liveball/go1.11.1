}

var benchmarks = []testing.InternalBenchmark{

}

var examples = []testing.InternalExample{

	{"ExampleNewReader", _xtest.ExampleNewReader, "Part \"one\": \"A section\"\nPart \"two\": \"And another\"\n", false},

}

func init() {
	testdeps.ImportPath = "mime/multipart"
}



func main() {

	m := testing.MainStart(testdeps.TestDeps{}, tests, benchmarks, examples)

	os.Exit(m.Run())

}

