	{"TestVariousTextLineEndings", _test.TestVariousTextLineEndings},

	{"TestLineLimit", _test.TestLineLimit},

	{"TestMultipartTruncated", _test.TestMultipartTruncated},

	{"TestMultipartStreamReadahead", _test.TestMultipartStreamReadahead},

	{"TestLineContinuation", _test.TestLineContinuation},

	{"TestQuotedPrintableEncoding", _test.TestQuotedPrintableEncoding},

	{"TestNested", _test.TestNested},

