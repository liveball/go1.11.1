	"testing/internal/testdeps"


	_test "mime/multipart"


	_xtest "mime/multipart_test"


)

var tests = []testing.InternalTest{

	{"TestReadForm", _test.TestReadForm},

	{"TestReadFormWithNamelessFile", _test.TestReadFormWithNamelessFile},

	{"TestReadFormWithTextContentType", _test.TestReadFormWithTextContentType},

