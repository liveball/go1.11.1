
package main

import (

	"os"

	"testing"
	"testing/internal/testdeps"


	_test "mime/quotedprintable"


	_xtest "mime/quotedprintable_test"


)

var tests = []testing.InternalTest{

	{"TestReader", _test.TestReader},

	{"TestExhaustive", _test.TestExhaustive},

	{"TestWriter", _test.TestWriter},

	{"TestWriterBinary", _test.TestWriterBinary},

	{"TestRoundTrip", _test.TestRoundTrip},

}

var benchmarks = []testing.InternalBenchmark{

	{"BenchmarkWriter", _test.BenchmarkWriter},

}

var examples = []testing.InternalExample{

	{"ExampleNewReader", _xtest.ExampleNewReader, "Hello, Gophers! <nil>\ninvalid escape: <b style=\"font-size: 200%\">hello</b> <nil>\nHello, Gophers! This symbol will be unescaped: = and this will be written in one line. <nil>\n", false},

	{"ExampleNewWriter", _xtest.ExampleNewWriter, "These symbols will be escaped: =3D =09\n", false},

}

func init() {
	testdeps.ImportPath = "mime/quotedprintable"
}



func main() {

	m := testing.MainStart(testdeps.TestDeps{}, tests, benchmarks, examples)

	os.Exit(m.Run())

}

