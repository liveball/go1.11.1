
package main

import (

	"os"

	"testing"
	"testing/internal/testdeps"


	_test "encoding/base64"


	_xtest "encoding/base64_test"


)

var tests = []testing.InternalTest{

	{"TestEncode", _test.TestEncode},

	{"TestEncoder", _test.TestEncoder},

	{"TestEncoderBuffering", _test.TestEncoderBuffering},

	{"TestDecode", _test.TestDecode},

	{"TestDecoder", _test.TestDecoder},

	{"TestDecoderBuffering", _test.TestDecoderBuffering},

	{"TestDecodeCorrupt", _test.TestDecodeCorrupt},

	{"TestEncodedLen", _test.TestEncodedLen},

	{"TestDecodedLen", _test.TestDecodedLen},

	{"TestBig", _test.TestBig},

	{"TestNewLineCharacters", _test.TestNewLineCharacters},

	{"TestDecoderIssue3577", _test.TestDecoderIssue3577},

	{"TestDecoderIssue4779", _test.TestDecoderIssue4779},

	{"TestDecoderIssue7733", _test.TestDecoderIssue7733},

	{"TestDecoderIssue15656", _test.TestDecoderIssue15656},

	{"TestDecoderRaw", _test.TestDecoderRaw},

}

var benchmarks = []testing.InternalBenchmark{

	{"BenchmarkEncodeToString", _test.BenchmarkEncodeToString},

	{"BenchmarkDecodeString", _test.BenchmarkDecodeString},

}

var examples = []testing.InternalExample{

	{"Example", _xtest.Example, "SGVsbG8sIOS4lueVjA==\nHello, 世界\n", false},

	{"ExampleEncoding_EncodeToString", _xtest.ExampleEncoding_EncodeToString, "YW55ICsgb2xkICYgZGF0YQ==\n", false},

	{"ExampleEncoding_DecodeString", _xtest.ExampleEncoding_DecodeString, "\"some data with \\x00 and \\ufeff\"\n", false},

	{"ExampleNewEncoder", _xtest.ExampleNewEncoder, "Zm9vAGJhcg==\n", false},

}

func init() {
	testdeps.ImportPath = "encoding/base64"
}



func main() {

	m := testing.MainStart(testdeps.TestDeps{}, tests, benchmarks, examples)

	os.Exit(m.Run())

}

