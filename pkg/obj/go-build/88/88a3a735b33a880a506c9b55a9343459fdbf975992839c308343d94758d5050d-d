
package main

import (

	"os"

	"testing"
	"testing/internal/testdeps"


	_test "net/http/httptest"


	_xtest "net/http/httptest_test"


)

var tests = []testing.InternalTest{

	{"TestNewRequest", _test.TestNewRequest},

	{"TestRecorder", _test.TestRecorder},

	{"TestServer", _test.TestServer},

	{"TestServerZeroValueClose", _test.TestServerZeroValueClose},

}

var benchmarks = []testing.InternalBenchmark{

}

var examples = []testing.InternalExample{

	{"ExampleResponseRecorder", _xtest.ExampleResponseRecorder, "200\ntext/html; charset=utf-8\n<html><body>Hello World!</body></html>\n", false},

	{"ExampleServer", _xtest.ExampleServer, "Hello, client\n", false},

	{"ExampleNewTLSServer", _xtest.ExampleNewTLSServer, "Hello, client\n", false},

}

func init() {
	testdeps.ImportPath = "net/http/httptest"
}



func main() {

	m := testing.MainStart(testdeps.TestDeps{}, tests, benchmarks, examples)

	os.Exit(m.Run())

}

