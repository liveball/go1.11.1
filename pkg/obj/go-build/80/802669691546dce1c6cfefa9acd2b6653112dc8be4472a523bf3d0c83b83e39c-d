
package main

import (

	"os"

	"testing"
	"testing/internal/testdeps"


	_test "encoding/pem"


	_xtest "encoding/pem_test"


)

var tests = []testing.InternalTest{

	{"TestGetLine", _test.TestGetLine},

	{"TestDecode", _test.TestDecode},

	{"TestBadDecode", _test.TestBadDecode},

	{"TestEncode", _test.TestEncode},

	{"TestLineBreaker", _test.TestLineBreaker},

	{"TestFuzz", _test.TestFuzz},

	{"TestBadEncode", _test.TestBadEncode},

}

var benchmarks = []testing.InternalBenchmark{

	{"BenchmarkEncode", _test.BenchmarkEncode},

	{"BenchmarkDecode", _test.BenchmarkDecode},

}

var examples = []testing.InternalExample{

	{"ExampleDecode", _xtest.ExampleDecode, "Got a *rsa.PublicKey, with remaining data: \"and some more\"\n", false},

	{"ExampleEncode", _xtest.ExampleEncode, "-----BEGIN MESSAGE-----\nAnimal: Gopher\n\ndGVzdA==\n-----END MESSAGE-----\n", false},

}

func init() {
	testdeps.ImportPath = "encoding/pem"
}



func main() {

	m := testing.MainStart(testdeps.TestDeps{}, tests, benchmarks, examples)

	os.Exit(m.Run())

}

