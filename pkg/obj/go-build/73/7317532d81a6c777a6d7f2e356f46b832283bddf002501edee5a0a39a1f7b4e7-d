
package main

import (

	"os"

	"testing"
	"testing/internal/testdeps"


	_test "net/mail"


	_xtest "net/mail_test"


)

var tests = []testing.InternalTest{

	{"TestParsing", _test.TestParsing},

	{"TestDateParsing", _test.TestDateParsing},

	{"TestAddressParsingError", _test.TestAddressParsingError},

	{"TestAddressParsing", _test.TestAddressParsing},

	{"TestAddressParser", _test.TestAddressParser},

	{"TestAddressString", _test.TestAddressString},

	{"TestAddressParsingAndFormatting", _test.TestAddressParsingAndFormatting},

	{"TestAddressFormattingAndParsing", _test.TestAddressFormattingAndParsing},

}

var benchmarks = []testing.InternalBenchmark{

}

var examples = []testing.InternalExample{

	{"ExampleParseAddressList", _xtest.ExampleParseAddressList, "Alice alice@example.com\nBob bob@example.com\nEve eve@example.com\n", false},

	{"ExampleParseAddress", _xtest.ExampleParseAddress, "Alice alice@example.com\n", false},

	{"ExampleReadMessage", _xtest.ExampleReadMessage, "Date: Mon, 23 Jun 2015 11:40:36 -0400\nFrom: Gopher <from@example.com>\nTo: Another Gopher <to@example.com>\nSubject: Gophers at Gophercon\nMessage body\n", false},

}

func init() {
	testdeps.ImportPath = "net/mail"
}



func main() {

	m := testing.MainStart(testdeps.TestDeps{}, tests, benchmarks, examples)

	os.Exit(m.Run())

}

