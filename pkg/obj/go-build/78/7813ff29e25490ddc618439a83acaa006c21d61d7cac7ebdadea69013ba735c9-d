	"testing"
	"testing/internal/testdeps"


	_test "crypto/sha512"



)

var tests = []testing.InternalTest{

	{"TestGolden", _test.TestGolden},

	{"TestGoldenMarshal", _test.TestGoldenMarshal},

	{"TestMarshalMismatch", _test.TestMarshalMismatch},

	{"TestSize", _test.TestSize},

	{"TestBlockSize", _test.TestBlockSize},

	{"TestBlockGeneric", _test.TestBlockGeneric},

}

var benchmarks = []testing.InternalBenchmark{

	{"BenchmarkHash8Bytes", _test.BenchmarkHash8Bytes},

	{"BenchmarkHash1K", _test.BenchmarkHash1K},

	{"BenchmarkHash8K", _test.BenchmarkHash8K},

}

var examples = []testing.InternalExample{

}

func init() {
	testdeps.ImportPath = "crypto/sha512"
}



func main() {

	m := testing.MainStart(testdeps.TestDeps{}, tests, benchmarks, examples)

	os.Exit(m.Run())

