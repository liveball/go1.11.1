
package main

import (

	"os"

	"testing"
	"testing/internal/testdeps"


	_test "net/http/httputil"


	_xtest "net/http/httputil_test"


)

var tests = []testing.InternalTest{

	{"TestDumpRequest", _test.TestDumpRequest},

	{"TestDumpResponse", _test.TestDumpResponse},

	{"TestReverseProxy", _test.TestReverseProxy},

	{"TestReverseProxyStripHeadersPresentInConnection", _test.TestReverseProxyStripHeadersPresentInConnection},

	{"TestXForwardedFor", _test.TestXForwardedFor},

	{"TestReverseProxyQuery", _test.TestReverseProxyQuery},

	{"TestReverseProxyFlushInterval", _test.TestReverseProxyFlushInterval},

	{"TestReverseProxyCancelation", _test.TestReverseProxyCancelation},

	{"TestNilBody", _test.TestNilBody},

	{"TestUserAgentHeader", _test.TestUserAgentHeader},

	{"TestReverseProxyGetPutBuffer", _test.TestReverseProxyGetPutBuffer},

	{"TestReverseProxy_Post", _test.TestReverseProxy_Post},

	{"TestReverseProxy_NilBody", _test.TestReverseProxy_NilBody},

	{"TestReverseProxyModifyResponse", _test.TestReverseProxyModifyResponse},

	{"TestReverseProxyErrorHandler", _test.TestReverseProxyErrorHandler},

	{"TestReverseProxy_CopyBuffer", _test.TestReverseProxy_CopyBuffer},

	{"TestServeHTTPDeepCopy", _test.TestServeHTTPDeepCopy},

	{"TestClonesRequestHeaders", _test.TestClonesRequestHeaders},

	{"TestModifyResponseClosesBody", _test.TestModifyResponseClosesBody},

	{"TestReverseProxy_PanicBodyError", _test.TestReverseProxy_PanicBodyError},

}

var benchmarks = []testing.InternalBenchmark{

	{"BenchmarkServeHTTP", _test.BenchmarkServeHTTP},

}

var examples = []testing.InternalExample{

	{"ExampleDumpRequest", _xtest.ExampleDumpRequest, "\"POST / HTTP/1.1\\r\\nHost: www.example.org\\r\\nAccept-Encoding: gzip\\r\\nContent-Length: 75\\r\\nUser-Agent: Go-http-client/1.1\\r\\n\\r\\nGo is a general-purpose language designed with systems programming in mind.\"\n", false},

	{"ExampleDumpRequestOut", _xtest.ExampleDumpRequestOut, "\"PUT / HTTP/1.1\\r\\nHost: www.example.org\\r\\nUser-Agent: Go-http-client/1.1\\r\\nContent-Length: 75\\r\\nAccept-Encoding: gzip\\r\\n\\r\\nGo is a general-purpose language designed with systems programming in mind.\"\n", false},

	{"ExampleDumpResponse", _xtest.ExampleDumpResponse, "\"HTTP/1.1 200 OK\\r\\nContent-Length: 76\\r\\nContent-Type: text/plain; charset=utf-8\\r\\nDate: Wed, 19 Jul 1972 19:00:00 GMT\\r\\n\\r\\nGo is a general-purpose language designed with systems programming in mind.\\n\"\n", false},

	{"ExampleReverseProxy", _xtest.ExampleReverseProxy, "this call was relayed by the reverse proxy\n", false},

}

func init() {
	testdeps.ImportPath = "net/http/httputil"
}



func main() {

	m := testing.MainStart(testdeps.TestDeps{}, tests, benchmarks, examples)

	os.Exit(m.Run())

}

