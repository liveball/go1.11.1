
package main

import (

	"os"

	"testing"
	"testing/internal/testdeps"


	_test "crypto/ecdsa"


	_ "crypto/ecdsa_test"


)

var tests = []testing.InternalTest{

	{"TestKeyGeneration", _test.TestKeyGeneration},

	{"TestSignAndVerify", _test.TestSignAndVerify},

	{"TestNonceSafety", _test.TestNonceSafety},

	{"TestINDCCA", _test.TestINDCCA},

	{"TestVectors", _test.TestVectors},

	{"TestNegativeInputs", _test.TestNegativeInputs},

	{"TestZeroHashSignature", _test.TestZeroHashSignature},

}

var benchmarks = []testing.InternalBenchmark{

	{"BenchmarkSignP256", _test.BenchmarkSignP256},

	{"BenchmarkSignP384", _test.BenchmarkSignP384},

	{"BenchmarkVerifyP256", _test.BenchmarkVerifyP256},

	{"BenchmarkKeyGeneration", _test.BenchmarkKeyGeneration},

}

var examples = []testing.InternalExample{

}

func init() {
	testdeps.ImportPath = "crypto/ecdsa"
}



func main() {

	m := testing.MainStart(testdeps.TestDeps{}, tests, benchmarks, examples)

	os.Exit(m.Run())

}

