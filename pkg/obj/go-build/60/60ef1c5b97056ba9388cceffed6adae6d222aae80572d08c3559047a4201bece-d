
package main

import (

	"os"

	"testing"
	"testing/internal/testdeps"


	_test "io/ioutil"


	_xtest "io/ioutil_test"


)

var tests = []testing.InternalTest{

	{"TestReadFile", _test.TestReadFile},

	{"TestWriteFile", _test.TestWriteFile},

	{"TestReadDir", _test.TestReadDir},

	{"TestTempFile", _test.TestTempFile},

	{"TestTempFile_pattern", _test.TestTempFile_pattern},

	{"TestTempDir", _test.TestTempDir},

	{"TestTempDir_BadDir", _test.TestTempDir_BadDir},

}

var benchmarks = []testing.InternalBenchmark{

}

var examples = []testing.InternalExample{

	{"ExampleReadAll", _xtest.ExampleReadAll, "Go is a general-purpose language designed with systems programming in mind.\n", false},

	{"ExampleReadFile", _xtest.ExampleReadFile, "File contents: Hello, Gophers!\n", false},

}

func init() {
	testdeps.ImportPath = "io/ioutil"
}



func main() {

	m := testing.MainStart(testdeps.TestDeps{}, tests, benchmarks, examples)

	os.Exit(m.Run())

}

